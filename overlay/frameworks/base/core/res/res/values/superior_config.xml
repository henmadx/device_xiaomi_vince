<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Rounded corner radius -->
    <dimen name="rounded_corner_radius">32px</dimen>

    <!-- Whether device has screen with higher aspect ratio -->
    <bool name="config_haveHigherAspectRatioScreen">true</bool>

    <!-- SystemInfo Sys Nodes -->
    <bool name="config_deviceSupportSysInfo">true</bool>
    <integer name="config_sysBatteryTempMultiplier">10</integer>
    <integer name="config_sysCPUTempMultiplier">1000</integer>
    <string name="config_sysBatteryTemp">/sys/class/power_supply/battery/temp</string>
    <string name="config_sysCPUTemp">/sys/class/thermal/thermal_zone0/temp</string>
    <string name="config_sysGPUFreq">/sys/kernel/gpu/gpu_clock</string>
    <string name="config_sysGPULoad">/sys/kernel/gpu/gpu_busy</string>

    <!-- Smart charging battery percentage -->
    <bool name="config_deviceSupportSmartCharging">true</bool>
    <integer name="config_smartChargingBatteryLevel">80</integer>
    <integer name="config_smartChargingBatteryResumeLevel">60</integer>
    <string name="config_SmartChargingSysfsNode" translatable="false">/sys/class/power_supply/battery/battery_charging_enabled</string>
    <string name="config_SmartChargingSupspendValue" translatable="false">0</string>
    <string name="config_SmartChargingResumeValue" translatable="false">1</string>
</resources>
